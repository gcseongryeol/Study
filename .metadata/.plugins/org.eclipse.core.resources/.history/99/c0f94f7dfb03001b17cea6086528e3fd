package question_for;

import java.util.Scanner;

public class _2439 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int num = scan.nextInt();
		
		for(int i = 0; i <= num; i++ ) {
			for(int j = 0; j < i; j++) {
				System.out.printf("%nums", "*");
			} System.out.println();
		} 
		
	}

}
