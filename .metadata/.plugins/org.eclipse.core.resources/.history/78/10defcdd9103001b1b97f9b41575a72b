package question_for;

import java.util.Scanner;

public class _15552 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		
		for(int i = 0; i < scan.nextInt(); i++) {
			System.out.println(a+b);
		}
		
		scan.close();
	}
	
	

}
