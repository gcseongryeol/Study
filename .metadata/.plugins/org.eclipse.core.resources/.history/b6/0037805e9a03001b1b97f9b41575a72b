package question_for;

import java.util.Scanner;

public class _11021 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		
		for(int i = 1; i < n; i++) {
			
			int a = scan.nextInt(10);
			int b = scan.nextInt(10);
			System.out.printf("Case #%d: " + (a+b), i+1 );
			
			
		}
	}

}
