package question_for;

import java.util.Scanner;

public class _2739 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		
		for(int N = 1; N < 10; N++) {
			for(int i = 1; i < 10; i++) {
				N = scan.nextInt();
				System.out.println(N + "*" + i + "=" + N * i );
			}
		}
		
	}

}
