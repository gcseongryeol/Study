package question_for;

import java.util.Scanner;

public class _11021 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		
		for(int i =0; i < n; i++) {
			
			byte a = scan.nextByte();
			byte b = scan.nextByte();
			System.out.println(int)("Case #%d:" + (a+b), i );
			
			
		}
	}

}
